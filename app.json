{
  "expo": {
    "name": "Safe Space",
    "slug": "safe_space",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/safe-space-icon.png",
    "splash": {
      "image": "./assets/splash-screen-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/safe-space-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/safe-space-icon.png"
    }
  }
}
